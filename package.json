  "license": "Apache-2.0",
  "devDependencies": {
    "eslint": "^4.15.0",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^4.0.1",
    "polymer-cli": "^1.7.7"
  }
}
